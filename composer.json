{
    "name": "allocine/titania",
    "description": "Titania: constrained objects in php",
    "homepage": "https://github.com/allocine/titania",
    "keywords": [
        "datamapper",
        "mapper",
        "model",
        "entity",
        "json"
    ],
    "support": {
        "issues": "https://github.com/allocine/titania/issues",
        "source": "https://github.com/allocine/titania",
        "docs": "https://github.com/allocine/titania/blob/master/README.md"
    },
    "type": "library",
    "license": "Apache-2.0",
    "require": {
        "php": "^7.1",
        "symfony/options-resolver": "^2.7|^3.0|^4.0|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4" : {
            "Allocine\\Titania\\": "src/",
            "Tests\\Titania\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
